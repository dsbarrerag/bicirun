<div class="row">
  <div class="text-center">
    <div class="col-md-10" style="padding: 10px 0px 0px 0px;">
      <h1 style="font-size:200%;">Registro</h1>
    </div>
  </div>
  <div class="col-sm-10" style="padding: 0px 0px 20px 0px;">
    <div><hr class="style13"></div>
    <div class="form-basic">
      <div class="login-form" style="width: 100%">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="text-center">
          <h4>Iniciar sesión</h4>
          <div><hr class="style13"></div>
        </div>
        <div class="sign-in-htm">

          <div class="group" style="padding: 20px 0px 20px 0px">
            <label for="user" class="label">Usuario</label>
            <%= f.text_field :username, :id=>"user", :type=>"text", :class=>"input",autofocus: true %>
          </div>

          <div class="group" style="padding: 20px 0px 20px 0px">
            <label for="pass" class="label">Contraseña</label>
            <%= f.password_field :password,:id=>"pass", :type=>"password", :class=>"input", autocomplete: "off" %>
          </div>

          <div class="group" style="padding: 20px 0px 20px 0px">
            <div class="actions">
              <%= f.submit "Ingresar", :class=>"button"%>
            </div>
            <% end %>
          </div>

        </div>
      </div>
    </div>
  </div>
</div>
